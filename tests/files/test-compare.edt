@default_field("op")

if "==" then
	{comp x y} = (x == y)
	{comp x num(y)} = (x == num(y))
	{comp 1 y} = 1 == y
fi

if "!=" then
	{comp x y} = (x != y)
	{comp x num(y)} = (x != num(y))
	{comp 1 y} = 1 != y
fi

if "<" then
	{comp x y} = (x < y)
	{comp x num(y)} = (x < num(y))
	{comp 1 y} = 1 < y
fi

if "<=" then
	{comp x y} = (x <= y)
	{comp x num(y)} = (x <= num(y))
	{comp 1 y} = 1 <= y
fi

if ">" then
	{comp x y} = (x > y)
	{comp x num(y)} = (x > num(y))
	{comp 1 y} = 1 > y
fi

if ">=" then
	{comp x y} = (x >= y)
	{comp x num(y)} = (x >= num(y))
	{comp 1 y} = 1 >= y
fi
